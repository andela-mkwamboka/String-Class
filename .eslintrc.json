{
  "extends": "airbnb"
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "prototypejs": true,
    "es6": true
  },
  "globals": {},
  "ecmaFeatures": {
    "arrowFunctions": true,
    "classes": true
  },
  "parser": null,
  "rules": {
    "space-before-function-paren": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "indent": 2
  }
}